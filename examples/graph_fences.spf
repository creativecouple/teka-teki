      dick
      edge e1 get 0 get edge e1 get 1 get moveto
      edge e2 get 0 get edge e2 get 1 get lineto
      stroke
    } {
      gsave
        0.02 setlinewidth [0.05 0.2] 0.01 setdash
        edge e1 get 0 get edge e1 get 1 get moveto
        edge e2 get 0 get edge e2 get 1 get lineto
        stroke
      grestore
    } ifelse
  } for

