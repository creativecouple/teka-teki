/pgsave save def 200 dict begin
/sc 22.8 def
/pt {sc div} def
sc dup scale 0.5 pt dup translate
/duenn { 0.4 pt setlinewidth } def
/dick { 1 pt setlinewidth } def

% create arukone: -s 6
<<
/type (arukone)
/sol false
/X 6
/Y 6
/puzzle [
(+-+-+-+-+-+-+)
(|           |)
(+ + + + + + +)
(|  D     D  |)
(+ + + + + + +)
(|           |)
(+ + + + + + +)
(|    C      |)
(+ + + + + + +)
(|          A|)
(+ + + + + + +)
(|  A B C   B|)
(+-+-+-+-+-+-+) ]
/description [
 (Gr&ouml;&szlig;e 6x6)
 (4 Buchstaben)
]
>> currentdict copy pop

/e { exch def } bind def
/l { 4 2 roll moveto lineto stroke } def
/c { dup 4 2 roll moveto
     stringwidth pop 2 div neg 0.5 add 0.25 rmoveto show } def

1 setgray
0 0 X Y rectfill
0 setgray

dick
0 0 X Y rectstroke

duenn
0 1 X { /i e i 0 i Y l } for
0 1 Y { /j e 0 j X j l } for

sol {
  2 setlinecap
  0.5 setgray
  2 pt setlinewidth

  0 1 X { /i e
    0 1 Y 1 sub { /j e
      puzzle Y j sub 1 sub 2 mul 1 add get i 2 mul get (-) 0 get eq {
        i 0.5 sub j 0.5 add i 0.5 add j 0.5 add l
      } if
    } for
  } for

  0 1 X 1 sub { /i e
    0 1 Y { /j e
      puzzle Y j sub 2 mul get i 2 mul 1 add get (|) 0 get eq {
        i 0.5 add j 0.5 sub i 0.5 add j 0.5 add l
      } if
    } for
  } for
  0 setgray
} if

/Helvetica-Bold findfont 0.65 scalefont setfont
0 1 X 1 sub { /i e
  0 1 Y 1 sub { /j e
    puzzle Y j sub 1 sub 2 mul 1 add get i 2 mul 1 add get /h e
    h ( ) 0 get ne {
      i j h ( ) dup 0 3 index put exch pop c
    } if
  } for
} for

end pgsave restore showpage
